set terminal pdf
set log xy
set pointsize 0.5
set xrange [10:50]
set yrange [10e-15:1]

Vol=14./3.*pi*1**3
mu0=4*pi*1.e-7
M=1

set output 'by.pdf'
set ylabel '|B_y|'
plot  'magfield.dat' u 5:(abs(($7*mu0)/(4*pi))) w lp t 'measured',\
mu0/4/pi*M*Vol/(x-0.5)**3 t 'single dipole',\
100*(x-0.5)*1.e-3 t 'line'

show output
