set terminal pdf

set log xy
set pointsize 0.5
set autoscale xy
set xrange [0.5:]
#set yrange [1.e-20:1.e-5]
set format y "%.0e"


#set xtics 0,0.5,2

Vol=(4./3.)*pi*1**3
mu0=4*pi*1.e-7
Cm=1.e-7 
M=1.
nt2t=1.e-9


set title font "Arial,16"  'dipole benchmark' 
set output 'by.pdf'
set xlabel font "arial,12" 'distance to sphere'
set ylabel font "arial,12" '|B_y|'
plot 'magfield.dat' u ($5):(abs($7)) w lp t 'computed' ,\
Cm*M*Vol*((x)**-3) w lp t 'single dipole analytical'




