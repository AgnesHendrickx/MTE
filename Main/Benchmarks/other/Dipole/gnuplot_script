set term pdf enhanced
set log xy
set pointsize 0.5
Vol=4./3.*pi*50**3
mu0=4*pi*1.e-7
M=1

set xlabel 'distance to sphere'
set output 'Ax.pdf'
set ylabel 'A_x'
plot[][]\
 'magfield_along_a_line.ascii' u ($1-250):5 w lp t 'measured',\
mu0/4/pi*M*Vol/(x+250)**2 t 'single dipole'
 
#unset log xy
set output 'By.pdf'
set ylabel '|B_y|'
plot[][]\
 'magfield_along_a_line.ascii' u ($1-250):(abs($3)) w lp t 'measured',\
mu0/4/pi*M*Vol/(x+250)**3 t 'single dipole'
